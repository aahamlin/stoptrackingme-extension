{
    "name": "stoptrackingme-extension",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "webpack --mode=production",
        "start": "webpack-dev-server --mode=development --port=3000",
        "test": "karma start --single-run --browsers ChromiumHeadless karma.conf.js",
        "test:chrome": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
        "test:firefox": "karma start --single-run --browsers FirefoxHeadless karma.conf.js",
        "dev:debug": "karma start --debug --single-run=false --browsers Chromium karma.conf.js",
        "dev:watch": "karma start --auto-watch --debug --browsers ChromiumHeadless karma.conf.js"
    },
    "author": "Andrew Hamlin <aahamlin@gmail.com>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/aahamlin/stoptrackingme-extension.git"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "6.0.2",
        "elm": "0.19.1-3",
        "elm-webpack-loader": "6.0.1",
        "file-loader": "6.0.0",
        "karma": "5.0.9",
        "karma-chai": "0.1.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-firefox-launcher": "1.3.0",
        "karma-mocha": "2.0.1",
        "karma-opera-launcher": "1.0.0",
        "karma-sinon": "1.0.5",
        "mocha": "7.2.0",
        "puppeteer": "^3.3.0",
        "sinon": "9.0.2",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.11.0"
    }
}
