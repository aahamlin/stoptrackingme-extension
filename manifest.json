{
    "name": "My Extension",
    "description": "Block trackers using DisconnectMe data.",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16x16.png",
        "48": "icons/icon48x48.png",
        "128": "icons/icon128x128.png"
    },
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_title": "Event history",
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "storage",
        "https://*/*",
        "http://*/*"
    ]
}
